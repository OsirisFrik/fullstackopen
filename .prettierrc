{
  "plugins": [
    "prettier"
  ],
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "none",
  "printWidth": 80,
  "bracketSpacing": true,
  "importOrder": ["<THIRD_PARTY_MODULES>", "^@/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "jsxSingleQuote": true,
  "pluginSearchDirs": true,
  "importOrderParserPlugins": ["typescript", "tsx", "jsx"]
}
